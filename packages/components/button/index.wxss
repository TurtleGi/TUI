@font-face {
  font-family: "iconfont";
  /* Project id 3865415 */
  src: url('css/iconfont.woff2?t=1675407627181') format('woff2'), url('css/iconfont.woff/iconfont.woff?t=1675407627181') format('woff'), url('css/iconfont.ttf/iconfont.ttf?t=1675407627181') format('truetype');
}
Button {
  border: 1px solid transparent;
  padding: 0.6em 1.2em;
  margin-left: 10px;
  font-size: 1em;
  font-weight: 500;
  font-family: inherit;
  cursor: pointer;
  transition: border-color 0.25s;
}
.t-button-primary {
  color: #fff;
  background-color: #409eff;
  border-color: #409eff;
}
.t-button-primary:hover,
.t-button-primary:focus {
  background-color: #6dabea;
  border-color: #6dabea;
}
.t-button-success {
  color: #fff;
  background-color: #4f9e2c;
  border-color: #4f9e2c;
}
.t-button-success:hover,
.t-button-success:focus {
  background-color: #5ac22d;
  border-color: #5ac22d;
}
.t-button-warning {
  color: #fff;
  background-color: #c5852e;
  border-color: #c5852e;
}
.t-button-warning:hover,
.t-button-warning:focus {
  background-color: #ea9f37;
  border-color: #ea9f37;
}
.t-button-danger {
  color: #fff;
  background-color: #d05352;
  border-color: #d05352;
}
.t-button-danger:hover,
.t-button-danger:focus {
  background-color: #cc3030;
  border-color: #cc3030;
}
.t-button-info {
  color: #fff;
  background-color: #909399;
  border-color: #909399;
}
.t-button-info:hover,
.t-button-info:focus {
  background-color: #a6a9ad;
  border-color: #a6a9ad;
}
.disabled,
.disabled:hover,
.disabled:focus {
  cursor: not-allowed;
  background-color: #ebeef5;
  color: #c0c4cc;
  border-color: #fff;
}
.round,
.round:hover,
.round:focus {
  border-radius: 30px;
}
.icon,
.icon:hover,
.icon:focus {
  border-radius: 50%;
  width: 40px;
  height: 40px;
  padding: 0;
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.loading {
  position: relative;
}
.loading,
.loading:hover,
.loading:focus {
  background-color: #83c0ff;
  color: #fff;
  border-color: #83c0ff;
}
.loading::before {
  content: '';
  display: inline-block;
  width: 17px;
  height: 17px;
  position: absolute;
  left: 2px;
  top: 12px;
  background: url('images/loading.gif') no-repeat center center / 100%;
}
